APP_NAME="Military Promotion Exam System"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=https://your-domain.example.com

APP_LOCALE=th
APP_FALLBACK_LOCALE=th
APP_FAKER_LOCALE=th_TH

APP_MAINTENANCE_DRIVER=file

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=daily
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=warning

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=military_exam
DB_USERNAME=military_exam_user
DB_PASSWORD=change_me

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
CACHE_PREFIX=military_exam

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=smtp
MAIL_SCHEME=tls
MAIL_HOST=smtp.your-provider.com
MAIL_PORT=587
MAIL_USERNAME=mailer@your-domain.example.com
MAIL_PASSWORD=change_me
MAIL_FROM_ADDRESS=mailer@your-domain.example.com
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=ap-southeast-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# Spatie Laravel Backup
BACKUP_DISK=local
BACKUP_ARCHIVE_PASSWORD=
BACKUP_NOTIFICATION_EMAIL=ops@your-domain.example.com
BACKUP_MAX_AGE_DAYS=7
BACKUP_MAX_STORAGE_MB=10240
BACKUP_KEEP_ALL_DAYS=7
BACKUP_KEEP_DAILY_DAYS=14
BACKUP_KEEP_WEEKLY_WEEKS=8
BACKUP_KEEP_MONTHLY_MONTHS=6
BACKUP_KEEP_YEARLY_YEARS=2

VITE_APP_NAME="${APP_NAME}"
